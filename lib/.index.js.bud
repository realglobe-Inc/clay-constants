'use strict'

const apeTmpl = require('ape-tmpl')
const { description, name } = require('../package.json')

module.exports = apeTmpl.indexJsBud({
  desc: description,
  module: name,
  dirname: __dirname,
  subclasses: [
    '*'
  ]
})

if (!module.parent) {
  require('coz').render(__filename)
}
